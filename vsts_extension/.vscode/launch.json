    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "env": {
                "INPUT_SERVICEENDPOINT": "HYDRALAB",
                "ENDPOINT_URL_HYDRALAB": "",
                "ENDPOINT_AUTH_HYDRALAB": "{ \"parameters\": { \"apitoken\": \"\" } }",
                "INPUT_TEAMNAME": "Default",
                "INPUT_runningType": "INSTRUMENTATION",
                "INPUT_pkgPath": "",
                "INPUT_testPkgPath": "",
                "INPUT_pkgName": "",
                "INPUT_testPkgName": "",
                "INPUT_testSuiteClass": "",
                "INPUT_timeoutSec": "360",
                "INPUT_deviceIdentifier": "",
                "INPUT_instrumentationArgs": "Arg1=Value1, Value2; Arg2=Value3",
                "INPUT_attachmentsInfo": "[{\n\t\"fileName\": \"sample.txt\", \n\t\"filePath\": \"./sample.txt\",\n\t\"fileType\": \"COMMON\",\n\t\"loadType\": \"COPY\",\n\t\"loadDir\": \"attachments\"\n}]",
                "INPUT_runningInfo": "auto"
            },
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\Tasks\\HydraLabDeployTest\\hydra-lab-deploy-test.ts",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}